{
    "name": "Wilow Side Panel",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["side_panel_ContentScript.js"],
          "run_at": "document_end",
          "match_about_blank": true
        }
    ],
    "web_accessible_resources": [
        "side_panel.html"
    ]
}