{
    "name": "Navigation Logger",
    "version": "1.0",
    "description": "Exploring",
    "permissions": ["tabs", "storage", "history","contextMenus"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": ["content.js"]
        }
    ],"web_accessible_resources": [
       
        "overlay.html"
        
      ],
    "manifest_version": 2   
}
